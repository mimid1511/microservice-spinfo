generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Event {
  eventId        String   @id @default(auto()) @map("_id") @db.ObjectId
  name           String
  location       String
  maximum_places Int
  places_sold    Int
  price          Float
  tickets        String[]
}